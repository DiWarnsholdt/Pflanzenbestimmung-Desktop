<UserControl x:Class="Pflanzenbestimmung_Desktop.Administration"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Pflanzenbestimmung_Desktop"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Style="{DynamicResource AdministrationButtonsStyle}">
        <Button Content="Neue Pflanze anlegen"/>
        <Button Content="Zu lernende Kategorien"/>
        <Button Content="Vorhandene Nutzer verwalten"/>
        <Button Content="Zurück zum Hauptmenü"/>
    </StackPanel>
</UserControl>
