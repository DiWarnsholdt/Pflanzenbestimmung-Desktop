<UserControl x:Class="Pflanzenbestimmung_Desktop.Verwaltung"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Pflanzenbestimmung_Desktop"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Style="{DynamicResource HauptStyle}">
    <StackPanel Style="{DynamicResource EinstellungenButtonsStyle}">
        <Button Content="Neue Pflanze anlagen" />
        <Button Content="Zu lernende Kategorien" />
        <Button Content="Benutzer anlegen" />
        <Button Content="Vorhandene Benutzer verwalten" />
        <Button Content="Erscheinende Pflanzen einstellen" />

        <Button Content="Zurück zum Hauptmenü" />
    </StackPanel>
</UserControl>
