<UserControl x:Class="Pflanzenbestimmung_Desktop.QuizStatistik"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Pflanzenbestimmung_Desktop"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="800"
             Style="{DynamicResource HauptStyle}">
    <Grid>
        <ListView>
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

        <Button Name="HauptmenüButton" Click="HauptmenüButton_Click" Content="Hauptmenü" Width="200" Height="30" Margin="100,20" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Button Name="WeiterButton" Click="WeiterButton_Click" Content="Weiter" Width="200" Height="30" Margin="100,20" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
    </Grid>
</UserControl>
